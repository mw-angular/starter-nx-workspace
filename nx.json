{
  "npmScope": "starter-nx-workspace",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "website/website": {
      "tags": ["app:website/website"]
    },
    "website/website-e2e": {
      "tags": ["app:website/website-e2e"],
      "implicitDependencies": ["website/website"]
    },
    "styleguide/styleguide": {
      "tags": ["app:styleguide/styleguide"]
    },
    "styleguide/styleguide-e2e": {
      "tags": ["app:styleguide/styleguide-e2e"],
      "implicitDependencies": ["styleguide/styleguide"]
    },
    "frontend/design-system": {
      "tags": ["lib:frontend/design-system"]
    }
  }
}
