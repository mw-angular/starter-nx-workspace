{
  "name": "starter-nx-workspace",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "------------------ app: website/website ------------------": "",
    "develop:website": "nx serve website/website --port 4200",
    "develop-on-devices:website": "npm run start:website -- --host 0.0.0.0",
    "build:website": "npm run tailwindcss:website && nx build website/website --prod --stats-json",
    "tailwindcss:website": "nx run website/website:tailwindcss",
    "lint:website": "nx workspace-lint && nx lint website/website --fix && nx lint website/website-e2e --fix",
    "test:website": "nx test website/website",
    "e2e:website": "nx e2e website/website-e2e",
    "analyze:website": "webpack-bundle-analyzer dist/apps/website/website/stats.json",
    "------------------ lib: frontend/design-system ------------------": "",
    "lint:design-system": "nx workspace-lint && nx lint frontend/design-system --fix",
    "test:design-system": "nx test frontend/design-system",
    "------------------ common ------------------": "",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "lint-scss": "stylelint --max-warnings 0 --allow-empty-input --formatter verbose --fix \"{apps,libs}/**/*.scss\"",
    "lint-scss-ci": "stylelint --max-warnings 0 --allow-empty-input \"{apps,libs}/**/*.scss\"",
    "pre-commit": "./tools/hooks/pre-commit.sh",
    "pre-push": "./tools/hooks/pre-push.sh"
  },
  "engines": {
    "node": ">=14.15.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "commit-msg": "node ./tools/hooks/commit-msg.js",
      "pre-push": "npm run pre-push"
    }
  },
  "dependencies": {
    "@angular-devkit/build-angular": "~0.1100.1",
    "@angular-eslint/eslint-plugin": "~1.0.0",
    "@angular-eslint/eslint-plugin-template": "~1.0.0",
    "@angular-eslint/template-parser": "~1.0.0",
    "@angular/animations": "^11.0.0",
    "@angular/cdk": "^11.0.4",
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/compiler-cli": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/forms": "^11.0.0",
    "@angular/language-service": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/router": "^11.0.0",
    "@mw-angular/tailwindcss-builder": "^1.1.0",
    "@nrwl/angular": "^11.1.5",
    "@nrwl/cli": "11.1.5",
    "@nrwl/cypress": "11.1.5",
    "@nrwl/eslint-plugin-nx": "11.1.5",
    "@nrwl/jest": "11.1.5",
    "@nrwl/nest": "^11.1.5",
    "@nrwl/tao": "11.1.5",
    "@nrwl/workspace": "11.1.5",
    "@tailwindcss/forms": "^0.2.1",
    "@types/jest": "26.0.8",
    "@types/node": "12.12.38",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "autoprefixer": "^10.2.1",
    "cypress": "^6.0.1",
    "dotenv": "6.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^31.0.7",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-unicorn": "^26.0.1",
    "husky": "^4.3.8",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "lint-staged": "^10.5.3",
    "postcss": "^8.2.4",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "rxjs": "~6.5.5",
    "stylelint": "^13.9.0",
    "stylelint-config-hudochenkov": "^5.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "tslib": "^2.0.0",
    "typescript": "~4.0.3",
    "webpack-bundle-analyzer": "^4.3.0",
    "zone.js": "^0.10.2"
  }
}
